<section>
  {{!-- <p>Welcome {{model.name}}</p> --}}
  {{this.session.isAuthenticated}}
  <div class='row'>
    {{#if (manager-only this.model.userRoles) }}
      {{#if this.managerEnabled}}
      <p class='col-md-3 text-center spacing'>
        <LinkTo @route='dashboard.groups' @tagName='button' @disabled=this.groupsDisabled class='btn btn-light launcher'>
          <FaIcon  @icon='users' @size='5x' /><br/>
          Memberships<br>
          (For Groups)
        </LinkTo>
      </p>
      {{else}}
      <p class='col-md-3 text-center spacing'>
        <a href="https://members.barbershop.org" class='btn btn-light launcher'>
          <FaIcon @icon='users' @size='5x' /><br/>
          Group and repertory management is now available via the Member Center.</a>
      </p>
      {{/if}}
    {{/if}}
    {{#if (is-admin this.model.userRoles)}}
    <p class='col-md-3 text-center spacing'>
      <LinkTo @route='dashboard.conventions' @tagName='button' @disabled={{this.assignmentsDisabled}}>
        <BsButton class='btn btn-light launcher'>
        <FaIcon @icon='legal' @size='5x' /><br/>
        Judging<br/>
        Assignments<br/>
        </BsButton>
      </LinkTo>
    </p>
    {{/if}}

    {{#if (is-judge this.model.userRoles)}}
      <p class='col-md-3 text-center spacing'>
        <button class='btn btn-light launcher' disabled="disabled">
        <FaIcon @icon='legal' @size='5x' /><br/>
        Judging<br/>
        Assignments<br/>
        (DISABLED)
        </button>
      </p>
    {{/if}}
  </div>
